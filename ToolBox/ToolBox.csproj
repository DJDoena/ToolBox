<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.0</TargetFrameworks>
		<Platforms>AnyCPU</Platforms>
		<OutputType>Library</OutputType>
		<RootNamespace>DoenaSoft.ToolBox</RootNamespace>
		<AssemblyName>DoenaSoft.ToolBox</AssemblyName>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<SignAssembly>True</SignAssembly>
		<AssemblyOriginatorKeyFile>ToolBox.snk</AssemblyOriginatorKeyFile>
		<Title>DoenaSoft.ToolBox</Title>
		<Company>Doena Soft.</Company>
		<Authors>DJ Doena</Authors>
		<Description>List of useful classes for XML serialization and Linq</Description>
		<Copyright>2016 - 2023</Copyright>
		<PackageProjectUrl>https://github.com/DJDoena/ToolBox</PackageProjectUrl>
		<PackageIcon>djdsoft.png</PackageIcon>
		<RepositoryUrl>https://github.com/DJDoena/ToolBox</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>XML-serialization; Linq</PackageTags>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Version>2.0.0</Version>
		<FileVersion>2.0.0</FileVersion>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\djdsoft.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

</Project>